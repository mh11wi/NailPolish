VITE_GLOSSY="1"
VITE_MATTE="2"

### Customizable Envars ###

# title of the application shown in the browser's tab
VITE_TITLE="Nail Polish App"

# name of the collector shown in the navbar
VITE_COLLECTOR="Foo"

# file extension of all images
VITE_EXTENSION=".jpg"

# primary color of the application; one of: blue, indigo, purple, pink, red, orange, yellow, green, teal, cyan
VITE_PRIMARY_COLOR="blue"

# color of the placeholder tile displayed before an image loads
VITE_PLACEHOLDER_COLOR="lightgrey"

# whether a switch should be displayed that toggles between glossy and matte images
VITE_FINISH_TOGGLE="false"